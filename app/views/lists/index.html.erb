<%= image_tag "cinema.jpeg", alt: "cinema", width: '100%', height: 550%>
<div class="container">
  <h1>My lists</h1>
  <div class="flex-container">
    <% @lists.each do |list| %>

    <div class="card-category">
      <%= link_to list.name.capitalize, list_path(list) %>
      <div class="new-bookmark">
        <%= link_to "New bookmark", new_list_bookmark_path(list) %>
      </div>
    </div>
    <% end %>
  </div>

</div>
